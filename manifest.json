{
    "manifest_version": 2,
    "name": "NoteMaker",
    "description": "Write notes and annotations without exiting the current webpage",       
    "version": "1.0",
    "minimum_chrome_version":"29",
    "background": { "scripts": ["background.js"], "persistent":false },
    "icons": {  
            "128":"./images/pic_14.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Cool Note Maker",
        "default-icon": "./images/pic_14.png"
    },
    "permissions":[
    "tabs", 
    "<all_urls>",
    "identity",
    "https://*.googleapis.com/*"
    ],
    "oauth2":{  
        "client_id":"397675070852-k2kmog18qi013s5mi3rjoirnakmorkop.apps.googleusercontent.com",

        "scopes":[  
            "https://www.googleapis.com/auth/documents"
        ]
    },
    "key": ""
}
